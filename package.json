{
  "name": "@takram/planck-renderer",
  "version": "0.2.1",
  "description": "",
  "repository": "takram-design-engineering/planck-renderer",
  "main": "./dist/planck-renderer.js",
  "module": "./dist/planck-renderer.module.js",
  "browser": {
    "./dist/planck-renderer.js": "./dist/planck-renderer.browser.js",
    "./dist/planck-renderer.module.js": "./dist/planck-renderer.browser.module.js"
  },
  "author": "Shota Matsuda",
  "license": "MIT",
  "homepage": "http://takram.com/projects/planck",
  "files": [
    "package.json",
    "README.md",
    "dist/planck-renderer.js",
    "dist/planck-renderer.module.js",
    "dist/planck-renderer.browser.js",
    "dist/planck-renderer.browser.module.js",
    "dist/planck-renderer.min.js",
    "src"
  ],
  "scripts": {
    "build": "npm-run-all --parallel build:node build:browser",
    "build:node": "rollup -c",
    "build:browser": "npm-run-all build:browser:compile build:browser:compress",
    "build:browser:compile": "rollup -c rollup.config.browser.js",
    "build:browser:compress": "uglifyjs --compress --mangle --output ${npm_package_main%.*}.min.js ${npm_package_main%.*}.browser.js",
    "test": "npm-run-all lint test:compile test:node test:saucelabs",
    "test:compile": "npm-run-all --parallel test:compile:rollup test:compile:webpack",
    "test:compile:rollup": "rollup -c rollup.config.test.js",
    "test:compile:webpack": "webpack --config webpack.config.test.js",
    "test:node": "mocha --recursive test/src",
    "test:browser": "browser-sync start --server . --files dist --startPath test --no-open --no-notify",
    "test:saucelabs": "mocha test/saucelabs.js",
    "lint": "npm-run-all lint:src lint:test lint:example",
    "lint:src": "standard -v \"*.js\" \"src/**/*.js\"",
    "lint:test": "standard -v --env mocha \"test/**/*.js\"",
    "lint:example": "standard -v --env mocha \"example/**/*.js\"",
    "lint-fix": "npm-run-all lint-fix:src lint-fix:test lint-fix:example",
    "lint-fix:src": "standard -v --fix \"*.js\" \"src/**/*.js\"",
    "lint-fix:test": "standard -v --fix --env mocha \"test/**/*.js\"",
    "lint-fix:example": "standard -v --fix --env mocha \"example/**/*.js\"",
    "prewatch": "npm-run-all build test:compile",
    "watch": "npm-run-all --parallel test:node watch:src watch:test watch:rollup watch:webpack test:browser",
    "watch:src": "nodemon -C --watch rollup.config.js --watch rollup.config.browser.js --watch src --exec \"npm-run-all build test:compile test:node\"",
    "watch:test": "nodemon -C --watch test --exec \"npm-run-all test:compile test:node\"",
    "watch:rollup": "nodemon -C --watch rollup.config.test.js --exec \"npm run test:compile:rollup\"",
    "watch:webpack": "nodemon -C --watch webpack.config.test.js --exec \"npm run test:compile:webpack\"",
    "clean": "trash dist/*",
    "example": "webpack-dev-server --config example/webpack.config.development.js",
    "example:compile": "webpack --config example/webpack.config.production.js"
  },
  "peerDependencies": {
    "@takram/planck-core": "0.x",
    "three": "^0.93.0"
  },
  "devDependencies": {
    "@shotamatsuda/rollup-plugin-glslify": "^0.1.2",
    "@takram/planck-core": "^0.7.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2016": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "babel-preset-stage-3": "^6.24.1",
    "browser-sync": "^2.24.4",
    "chai": "^4.1.2",
    "chalk": "^2.4.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.1",
    "eslint": "^4.19.1",
    "express": "^4.16.3",
    "mocha": "^5.2.0",
    "nodemon": "^1.17.5",
    "npm-run-all": "^4.1.3",
    "rollup": "^0.60.2",
    "rollup-plugin-babel": "^3.0.4",
    "rollup-plugin-commonjs": "~9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "sauce-connect-launcher": "^1.2.4",
    "saucelabs": "^1.5.0",
    "source-map-support": "^0.5.6",
    "standard": "^11.0.1",
    "three": "^0.93.0",
    "uglify-js": "^3.4.0",
    "webpack": "^4.12.0",
    "webpack-cli": "^3.0.4",
    "webpack-dev-server": "^3.1.4",
    "webpack-merge": "^4.1.2"
  },
  "saucelabs": {
    "framework": "mocha",
    "platforms": [
      [
        "Windows 10",
        "microsoftedge",
        "13.10586"
      ],
      [
        "Windows 10",
        "firefox",
        "29.0"
      ],
      [
        "Windows 10",
        "chrome",
        "49.0"
      ],
      [
        "Linux",
        "firefox",
        "29.0"
      ],
      [
        "OS X 10.11",
        "safari",
        "10.0"
      ],
      [
        "OS X 10.11",
        "firefox",
        "29.0"
      ],
      [
        "OS X 10.11",
        "chrome",
        "49.0"
      ]
    ]
  }
}
